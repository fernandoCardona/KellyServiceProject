<section class="contact">
<h1>Contact us!</h1>


    <form action="/contact" method="POST">

        <label>
            <input type="text" name="fullName" placeholder="Name" required/>
        </label>
        <label> 
            <input type="email" name="email" placeholder="Email" required/>
        </label>
        
        <label>
            <textarea name="textareaContact" id="textareaContact" cols="30" rows="10" required></textarea>
        </label>

        <button type="submit" id="btn-singup-Client" class="btn btn-Client-login">Signup</button>

        {{#if errorMessage}}
            <p class="error">{{errorMessage}}</p>
        {{/if}}
    </form>

</section>